{
  "name": "test-styled-jsx-package",
  "version": "0.0.1",
  "description": "components library with rollup and styled-jsx",
  "main": "esm/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run release",
    "release": "npm run build:dev && npm run build:prod",
    "build:dev": "NODE_ENV=development rollup -c",
    "clear": "rm -rf ./dist ./esm",
    "build:esm": "babel --config-file ./scripts/babel.config.js --extensions \".js,.ts,.tsx\" ./src --out-dir ./esm --ignore \"**/__tests__/**/*,**/*.d.ts,**/*.spec.ts\"",
    "build:types": "tsc",
    "build": "yarn clear && yarn build:esm && yarn build:types",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": "git+https://github.com/Tomekmularczyk/rollup-styled-jsx.git",
  "keywords": [
    "rollup",
    "styled-jsx",
    "components",
    "library",
    "package"
  ],
  "author": "Tomasz Mularczyk",
  "license": "ISC",
  "homepage": "https://github.com/Tomekmularczyk/rollup-styled-jsx#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.16",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/addons": "^6.0.22",
    "@storybook/react": "^6.0.22",
    "@types/styled-jsx": "^2.2.8",
    "babel-loader": "^8.1.0",
    "fs-extra": "^9.1.0",
    "klaw-sync": "^6.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.28.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "styled-jsx-plugin-sass": "^1.0.0",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "react": ">= 16.x.x"
  },
  "dependencies": {
    "node-sass": "^5.0.0",
    "styled-jsx": "3.2.5"
  }
}
