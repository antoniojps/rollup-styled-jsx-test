{
  "name": "test-styled-jsx-package",
  "version": "0.0.1",
  "description": "components library with rollup and styled-jsx",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run release",
    "release": "npm run build:dev && npm run build:prod",
    "build:dev": "NODE_ENV=development rollup -c",
    "clear": "rm -rf ./dist",
    "build:esm": "babel --config-file ./scripts/babel.config.js --extensions \".js,.ts,.tsx\" ./src --out-dir ./dist --ignore \"**/*.stories.tsx,**/*.spec.ts,**/*.spec.tsx,**/*.d.ts\"",
    "build:types": "tsc",
    "build": "yarn clear && yarn build:esm && yarn build:types",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": "git+https://github.com/Tomekmularczyk/rollup-styled-jsx.git",
  "keywords": [
    "rollup",
    "styled-jsx",
    "components",
    "library",
    "package"
  ],
  "author": "Tomasz Mularczyk",
  "license": "ISC",
  "homepage": "https://github.com/Tomekmularczyk/rollup-styled-jsx#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.16",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/addons": "^6.0.22",
    "@storybook/react": "^6.0.22",
    "@types/next-auth": "^3.1.24",
    "@types/react-select": "3.0.13",
    "@types/react-table": "^7.0.28",
    "@types/styled-jsx": "^2.2.8",
    "next": "10.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-jsx-plugin-sass": "^1.0.0",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "next": "10.0.1",
    "react": "16.13.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@geist-ui/react": "^2.1.0-canary.2",
    "@geist-ui/react-icons": "^1.0.1",
    "formik": "^2.2.6",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "lodash": "^4.17.20",
    "next-auth": "^3.4.1",
    "node-sass": "^5.0.0",
    "react-select": "3.1.0",
    "react-table": "^7.6.3",
    "styled-jsx": "3.2.5",
    "yup": "^0.32.8"
  }
}
